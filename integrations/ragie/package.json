{
  "name": "@mastra/ragie",
  "version": "1.2.11-alpha.2",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup --silent --config tsup.config.ts",
    "build:dev": "tsup src/index.ts --format esm,cjs --no-dts --clean --treeshake=smallest --splitting --watch && tsc -p tsconfig.build.json",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "clean": "rm -rf dist && rm -rf node_modules",
    "gen:zod:schema": "pnpx ts-to-zod  src/client/types.gen.ts src/client/zodSchema.ts"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@mastra/core": "workspace:*",
    "@microsoft/api-extractor": "^7.52.8",
    "@rollup/plugin-image": "^3.0.3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "@internal/types-builder": "workspace:*"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@hey-api/client-fetch": "^0.3.4",
    "ts-to-zod": "^3.15.0",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "@mastra/core": ">=0.10.7-0 <0.16.0-0"
  },
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "integrations/ragie"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  }
}
